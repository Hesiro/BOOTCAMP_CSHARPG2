<Window x:Class="WpfAppEventos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppEventos"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel x:Name="Panel" ButtonBase.Click="Panel_Click">
        <Button x:Name="button1" Content="Button 1" Width="100" Height="50" Margin="15" Click="button1_Click"/>
        <Button x:Name="button2" Content="Button 2" Width="100" Height="50" Margin="15" Click="button2_Click"/>
        <Button x:Name="button3" Content="Button 3" Width="100" Height="50" Margin="15" Click="button3_Click"/>
    </StackPanel>
</Window>
